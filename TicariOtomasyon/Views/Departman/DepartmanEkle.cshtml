@model TicariOtomasyon.Models.Siniflar.Departman
@{
    ViewBag.Title = "DepartmanEkle";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<br />
<h2>Yeni Departman Ekle</h2>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x=>x.DepartmanAdi)
    @Html.TextBoxFor(x => x.DepartmanAdi, new{@class="form-control"})
<br />
    <button class="btn btn-info">Kaydet</button>
</form>
